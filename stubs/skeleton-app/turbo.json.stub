{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "composer:install": {
      "cache": false,
      "outputs": ["vendor/**"]
    },
    "test": {
      "outputs": [".phpunit.cache/**"]
    },
    "lint": {
      "outputs": []
    },
    "format": {
      "outputs": []
    },
    "typecheck": {
      "outputs": [".phpstan.cache/**"]
    },
    "clean": {
      "cache": false
    }
  }
}
